{
  "name": "reminder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "tsc --noEmit && next lint --max-warnings=0",
    "lint:fix": "next lint --max-warnings=0 --fix",
    "stylelint:check": "stylelint \"**/*.{css,scss}\"",
    "stylelint:fix": "stylelint --fix \"**/*.{css,scss}\"",
    "test": "jest",
    "db:migrate": "dotenv -e .env.development.local prisma migrate dev",
    "db:client:refresh": "dotenv -e .env.development.local prisma generate",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@types/node": "20.5.9",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@vercel/postgres": "^0.5.1",
    "autoprefixer": "10.4.15",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "next": "13.4.19",
    "postcss": "8.4.29",
    "prisma": "^5.7.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "classnames": "^2.3.2",
    "dotenv-cli": "^7.3.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "sass": "^1.68.0",
    "stylelint": "^15.10.3",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-order": "^6.0.3"
  }
}
